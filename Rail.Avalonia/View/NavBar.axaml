<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="Rail.Avalonia.View.NavBar"
        Height="50"
        xmlns:v="clr-namespace:Rail.Avalonia.View">
    <StackPanel Orientation="Horizontal">
        <Button Content="Home" Command="{Binding MoveToHome}" />
        <Button Content="Progressing" Command="{Binding MoveToProgressing}" />
        <Button Content="Settings" Command="{Binding MoveToSettings}" />
    </StackPanel>
</UserControl>